<?xml version="1.0"?>
<package format="2">
  <name>bwi_scavenger</name>
  <version>0.3.12</version>
  <description>
    Robot scavenger hunt interfaces for the Building-Wide Intelligence
    project of the University of Texas at Austin.
  </description>

  <author email="shiqi.zhang6@gmail.com">Shiqi Zhang</author>
  <maintainer email="piyushk@gmail.com">Piyush Khandelwal</maintainer>
  <maintainer email="jack.oquin@gmail.com">Jack O'Quin</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>message_generation</build_depend>

  <depend>actionlib</depend>
  <depend>actionlib_msgs</depend>
  <depend>boost</depend>
  <depend>bwi_kr_execution</depend>
  <depend>bwi_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>image_transport</depend>
  <depend>move_base_msgs</depend>
  <depend>pcl_ros</depend>
  <depend>roscpp</depend>
  <depend>sound_play</depend>
  <depend>std_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>yaml-cpp</depend>

  <exec_depend>message_runtime</exec_depend>

</package>
