<?xml version="1.0"?>
<package format="2">
  <name>bwi_rqt_plugins</name>
  <version>0.3.12</version>
  <description>
    Displays a simple GUI to the user. The GUI is capable of displaying a simple
    message or image to the user, and ask a question. The message and question
    plugins are also written in this package.
  </description>

  <maintainer email="piyushk@gmail.com">Piyush Khandelwal</maintainer>
  <maintainer email="jack.oquin@gmail.com">Jack O'Quin</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/bwi_rqt_plugins</url>
  <url type="repository">https://github.com/utexas-bwi/bwi_comman.git</url>
  <url type="bugtracker">https://github.com/utexas-bwi/bwi_common/issues</url>

  <author email="piyushk@gmail.com">Piyush Khandelwal</author>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>bwi_msgs</depend>
  <depend>class_loader</depend>
  <depend>cv_bridge</depend>
  <depend>image_transport</depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>sensor_msgs</depend>

  <exec_depend>python_qt_binding</exec_depend>
  <exec_depend>qt_gui</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>

  <export>
    <rqt_gui plugin="${prefix}/python_plugins.xml"/>
    <rqt_gui plugin="${prefix}/cpp_plugins.xml"/>
  </export>

</package>
